#include <iostream>

using namespace std;

int main()
{
int n,m;
cout<<"Размер первой ячейки"<<endl;
cin>>n;
cout<<"Размер второй ячейки"<<endl;
cin>>m;
int a[n][m];
int b[n][m];
cout<<"Ввод элементов первого массива"<<endl;
for(int i=0;i<n;i++)
 for(int j=0;j<m;j++)
cin>>a[i][j];

cout<<"Ввод элементов второго массива"<<endl;
for(int i=0;i<n;i++)
  for(int j=0;j<m;j++)
cin>>b[i][j];

for(int i=0;i<n;i++)
{
  for(int j=0;j<m;j++)
cout<<a[i][j]+b[i][j]<<" ";
cout<< endl;
}



return 0;
}

